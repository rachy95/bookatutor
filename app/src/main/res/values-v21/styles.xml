<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="excelColor" parent="android:Theme.DeviceDefault.Light.NoActionBar.Fullscreen">
        <item name="android:colorPrimary">@color/ELPurple</item>
        <item name="android:navigationBarColor">@color/ELPurple</item>
        <item name="android:statusBarColor">@color/ELPurple</item>
        <item name="android:textColorPrimary">@color/niceBlue</item>
        <item name="android:textColorPrimaryInverse">@color/niceBlue</item>
        <item name="android:textColorSecondary">@color/niceBlue</item>
        <item name="android:textColorSecondaryInverse">@color/niceBlue</item>
        <item name="android:colorForeground">@color/ELPurple</item>
        <item name="android:colorPrimaryDark">@color/ELPurple</item>
        <item name="colorPrimary">@color/ELPurple</item>
        <item name="colorAccent">@color/ELPurple</item>
        <item name="android:colorAccent">@color/ELPurple</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:spinnerStyle">@style/Widget.AppCompat.Spinner.DropDown.ActionBar</item>
        <item name="android:statusBarColor">@color/ELPurple</item>
        <item name="android:navigationBarColor">@color/ELPurple</item>
    </style>
</resources>